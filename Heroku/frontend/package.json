{
	"name": "twisterrob-cinema-web-frontend",
	"description": "AngularJS frontend for twisterrob-cinema",
	"license": "UNLICENCED",
	"repository": "local",
	"scripts": {
		"preinstall": "echo Installing Frontend dependencies",
		"postinstall": "echo Installed Frontend dependencies",
		"build": "webpack --optimize-minimize --define process.env.NODE_ENV=\"production\" --progress",
		"start": "webpack --watch",
		"dev-server": "webpack-dev-server --progress --colors --port 2992 --inline --content-base ../deploy/static",
		"test": "npm run test:jshint",
		"test:jshint": "jshint --verbose --show-non-errors src/planner/scripts"
	},
	"dependencies": {
		"lodash": "2.4.1",
		"bootstrap": "3.3.7",
		"jquery": "1.12.4",
		"angular": "1.6.6",
		"angular-resource": "1.6.6",
		"angular-animate": "1.6.6",
		"angular-ui-bootstrap": "2.5.6",
		"moment": "2.19.3",
		"moment-timezone": "0.5.14",
		"moment-range": "3.0.3"
	},
	"devDependencies": {
		"webpack": "3.3.0",
		"webpack-dev-server": "2.5.1",
		"html-webpack-plugin": "2.29.0",
		"copy-webpack-plugin": "4.0.1",
		"extract-text-webpack-plugin": "3.0.0",
		"raw-loader": "0.5.1",
		"css-loader": "0.28.7",
		"style-loader": "0.19.0",
		"url-loader": "0.6.2",
		"file-loader": "1.1.5",

		"jshint": "2.9.5"
	}
}
